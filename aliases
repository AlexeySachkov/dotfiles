
# Directories listing
alias l='ls -lh --color --block-size=1024'
alias la='ls -lah --color --block-size=1024'

# Changing directory
alias ..='cd ..'
alias ...='cd ../..'
alias cd..='cd ..'

# Grep
alias grep='grep --color'
alias grp='grep --color'

# Vim
alias v='vim'

# Git
alias gti='git'

# tar.gz
alias untargz='tar -zxvf'
